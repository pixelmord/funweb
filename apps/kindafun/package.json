    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.14",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "svelte": "^3.56.0",
    "svelte-check": "^3.1.4",
    "svelte-preprocess": "^5.0.1",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
